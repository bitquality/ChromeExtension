{
  "name": "listen clicks with Port Object",
  "description": "Utility for capturing clicks with Port Object",
  "version": "1.3",
  "permissions": [
    "tabs",
    "activeTab",
    "https://*/*",
    "http://*/*",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/angularjs/1.6.10/angular.min.js; object-src 'self'",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Port Test 1.7"  },
  "manifest_version": 2
}
